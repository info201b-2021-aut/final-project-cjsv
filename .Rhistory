cement = round(sum(cement_co2, na.rm = TRUE), digits = 2),
coal = round(sum(coal_co2, na.rm = TRUE), digits = 2),
flaring = round(sum(flaring_co2, na.rm = TRUE), digits = 2),
gas = round(sum(gas_co2, na.rm = TRUE), digits = 2),
other = round(sum(other_industry_co2, na.rm = TRUE), digits = 2)
)
